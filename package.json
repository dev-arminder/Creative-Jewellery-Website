{
  "name": "frontend-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for Frontend Project",
  "main": "index.js",
  "scripts": {
    "frontend:development": "webpack serve --progress --config webpack.config.development.js",
    "frontend:build": "webpack --progress --config webpack.config.build.js",
    "backend:build": "node app.js",
    "backend:development": "nodemon app.js",
    "start": "concurrently --kill-others \"npm run backend:development\" \"npm run frontend:development\" "
  },
  "author": "Arminder Singh",
  "license": "ISC",
  "devDependencies": {
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.5.1",
    "copy-webpack-plugin": "^10.1.0",
    "css-loader": "^6.5.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "file-loader": "^6.2.0",
    "glslify-loader": "^2.0.0",
    "image-minimizer-webpack-plugin": "^3.0.1",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "include-media": "^1.4.10",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss-loader": "^6.2.1",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "terser-webpack-plugin": "^5.2.5",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@prismicio/client": "^5.1.0",
    "body-parser": "^1.19.1",
    "dotenv": "^10.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "prismic-dom": "^2.2.6",
    "pug": "^3.0.2"
  }
}
